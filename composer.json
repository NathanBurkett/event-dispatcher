{
  "name": "nathanburkett/event-dispatcher",
  "description": "PSR-14 Event Dispatcher implementation",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "NathanBurkett",
      "email": "nathan@nathanb.me"
    }
  ],
  "require": {
    "php": ">=7.2",
    "psr/event-dispatcher": "^1.0",
    "psr/container": "^1.0"
  },
  "require-dev": {
    "league/container": "^3.2",
    "nathanburkett/mesa-phpunit": "^0.1.0",
    "phpmd/phpmd": "^2.6",
    "phpstan/phpstan": "^0.11.6",
    "phpunit/phpunit": "^7.5",
    "squizlabs/php_codesniffer": "^3.4"
  },
  "autoload": {
    "psr-4": {
      "NathanBurkett\\EventDispatcher\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "NathanBurkett\\EventDispatcher\\Tests\\": "tests/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true
  }
}
